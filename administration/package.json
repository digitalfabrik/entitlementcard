    "build": "vite build",
    "codegen:gql": "graphql-codegen --config graphql-codegen.yml",
    "codegen:gql:dev": "graphql-codegen --config graphql-codegen.yml --watch",
    "codegen:pb": "buf generate",
    "check-circular-deps": "bash ./scripts/check_circular_deps.sh",
    "format": "eslint . --fix && prettier . --write",
    "lint": "eslint . && prettier . --check",
    "postinstall": "npm run codegen:pb && npm run codegen:gql",
    "start": "concurrently 'npm run start:dev' 'npm run codegen:gql:dev'",
    "start:dev": "vite --open",
    "start:ci": "vite --open=false",
    "test": "jest",
    "test:e2e": "npx playwright test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "ts:check": "tsc --build"
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      },
      "tsx": {
        "skipTypeImports": true
