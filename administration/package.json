{
  "name": "administration",
  "version": "2.1.1",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@blueprintjs/core": "^4.10.1",
    "@blueprintjs/datetime": "^4.3.12",
    "@blueprintjs/popover2": "^1.7.3",
    "@blueprintjs/select": "^4.6.6",
    "@bufbuild/protobuf": "0.3.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@zxing/library": "^0.19.1",
    "apollo-upload-client": "^17.0.0",
    "canonicalize": "^1.0.8",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "jasmine": "^4.5.0",
    "localforage": "^1.10.0",
    "notistack": "^2.0.8",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.4",
    "react-router-dom": "6.2.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5",
    "web-vitals": "^3.0.2"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "0.3.0",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/blob-stream": "^0.1.30",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "^5.1.26",
    "apollo": "^2.34.0",
    "eslint-config-prettier": "^8.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-webpack": "^5.0.0",
    "prettier": "^2.7.1",
    "puppeteer": "^19.6.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "set -ae; REACT_APP_APPLICATION_COMMIT=$(./application_commit.sh); react-scripts start",
    "build": "set -ae; REACT_APP_APPLICATION_COMMIT=$(./application_commit.sh); react-scripts build",
    "test": "karma start --config=karma.config.js --single-run",
    "eject": "react-scripts eject",
    "lint": "eslint . && prettier . --check",
    "format": "prettier . --write",
    "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
    "generate-protobuf": "protoc -I ../specs --es_out=src/generated --es_opt=target=ts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es ../specs/card.proto",
    "postinstall": "npm run generate-protobuf && npm run generate-graphql"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-prettier",
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "src/generated/**",
      "build/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  }
}
